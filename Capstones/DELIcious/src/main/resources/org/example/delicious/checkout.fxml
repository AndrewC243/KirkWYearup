<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="bpane" prefHeight="480.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.delicious.MainController">
   <bottom>
       <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
           <buttons>
               <Button mnemonicParsing="false" onAction="#addOns" text="Back" translateX="-15.0" ButtonBar.buttonData="YES" />
               <Button mnemonicParsing="false" onAction="#saveOrder" text="Save" translateX="-15.0" ButtonBar.buttonData="NO" />
               <Button mnemonicParsing="false" onAction="#cancelOrder" text="Cancel" translateX="15.0" ButtonBar.buttonData="LEFT" />
           </buttons>
       </ButtonBar>
   </bottom>
   <center>
      <HBox fx:id="hbox" prefHeight="100.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
